import org.springframework.boot.context.properties.ConfigurationProperties;
import org.springframework.boot.context.properties.bind.ConstructorBinding;

@ConfigurationProperties(prefix = "pkptuna.hazelcast")
public record HazelcastRouterProperties(
        Heartbeat heartbeat,
        Consumer consumer
) {
    public record Heartbeat(long timeoutMillis) {}
    public record Consumer(long pollingIntervalMillis) {}
}
