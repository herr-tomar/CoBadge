@Configuration
public class ProtoDeserializerConfig {

    @Bean
    public ProtobufDeserializer<DisputeLookupRequestDto> disputeLookupRequestDeserializer() {
        return new ProtobufDeserializer<>(DisputeLookupRequestDto.parser());
    }

    @Bean
    public ProtobufDeserializer<DisputeLookupResponseDto> disputeLookupResponseDeserializer() {
        return new ProtobufDeserializer<>(DisputeLookupResponseDto.parser());
    }
}
